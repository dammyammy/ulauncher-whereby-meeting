{
  "required_api_version": "^2.0.0",
  "name": "Whereby - Join a meeting",
  "description": "Quick way to join a Whereby meeting - default browser is required.",
  "developer_name": "Damilola Ogunmoye",
  "icon": "images/whereby_icon.png",
  "options": {
    "query_debounce": 0.1
  },
  "preferences": [
    {
      "id": "whereby_kw",
      "type": "keyword",
      "name": "Whereby",
      "description": "Keyword to call this extension",
      "default_value": "whereby"
    },
    {
      "id": "base_uri",
      "type": "text",
      "name": "Whereby Base URI",
      "description": "Whereby base URI",
      "default_value": "whereby.com"
    },
    {
      "id": "default_room",
      "type": "text",
      "name": "Default Room",
      "description": "Default Whereby Room",
      "default_value": "helloworld"
    }
  ]
}
